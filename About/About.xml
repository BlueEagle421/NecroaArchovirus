<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>

  <name>Ushankas Necroa Archovirus</name>
  <author>Ushanka</author>
  <packageId>Ushanka.NecroaArchovirus</packageId>
  <description>A highly configurable mod that adds a single new infectious disease: the Necroa Archovirus. It spreads via shamblers to anything they can penetrate with claws or teeth.</description>

  <supportedVersions>
    <li>1.6</li>
  </supportedVersions>

  <modDependencies>
    <li>
      <packageId>Ludeon.RimWorld.Anomaly</packageId>
      <displayName>Anomaly</displayName>
    </li>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
  </modDependencies>

  <loadAfter>
    <li>Ludeon.RimWorld</li>
    <li>Ludeon.RimWorld.Anomaly</li>
    <li>brrainz.harmony</li>
  </loadAfter>

</ModMetaData>